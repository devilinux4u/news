                        <div class="text-2xl text-center font-semibold" style="font-family: 'Itim', 'cursive'">
                            Admin Page
                        </div>
                    </div>

                    <div class="mt-5">
                        <div class="border-2 p-[2px] rounded-full w-fit mx-auto">
                            <div class="h-28 w-28 rounded-full overflow-hidden relative">
                                <img class="absolute top-0 left-0 w-full h-full object-cover transform hover:scale-105 transition duration-300"
                                    src="https://source.unsplash.com/random" />
                            </div>
                        </div>
                        <div class="text-center">
                            <h3 class="text-2xl" style="font-family: 'Itim', 'cursive'">
                                Raj Chaulagain
                            </h3>
                        </div>
                    </div>

                    <div class="mx-auto mt-14 pl-5 text-lg cursor-pointer">

                        <p @click="pass = !pass" class="flex items-center gap-2 hover:text-gray-800">
                            Change Password
                        </p>
                        <a href="/user/logout">
                            <p class="flex items-center gap-2 hover:text-gray-800">
                                Log Out
                            </p>
                        </a>
                    </div>

                </div>
                <%- include("./includes/pass.ejs") %>
            </div>
        </aside>


